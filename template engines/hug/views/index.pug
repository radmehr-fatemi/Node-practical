doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel="stylesheet", href="/style.css")
    body 

    include partials/_nav
    
    h2 #{title}
    
    a(href=link) title link 
    
    each user in users
        ul 
        li #{user.name}